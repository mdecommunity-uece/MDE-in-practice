<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated activitydiagram -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="activitydiagram.presentation.ActivitydiagramModelWizardID"
            name="%_UI_ActivitydiagramModelWizard_label"
            class="activitydiagram.presentation.ActivitydiagramModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ActivitydiagramModelFile.gif">
         <description>%_UI_ActivitydiagramModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated activitydiagram -->
      <editor
            id="activitydiagram.presentation.ActivitydiagramEditorID"
            name="%_UI_ActivitydiagramEditor_label"
            icon="icons/full/obj16/ActivitydiagramModelFile.gif"
            extensions="activitydiagram"
            class="activitydiagram.presentation.ActivitydiagramEditor"
            contributorClass="activitydiagram.presentation.ActivitydiagramActionBarContributor">
      </editor>
   </extension>

</plugin>
